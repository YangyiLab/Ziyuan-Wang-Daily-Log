keywords = {Affine gap penalty,Center star alignment,K-band,Multiple sequence alignment,affine gap penalty,center star alignment,k-band,multiple sequence alignment},
pages = {322--327},
title = {{A Novel Center Star Multiple Sequence Alignment Algorithm Based on Affine Gap Penalty and K-Band}},
